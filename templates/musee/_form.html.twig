<div class="m-4">
    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-3">{{ form_row(form.numMus) }}</div>
        <div class="col-md-3">{{ form_row(form.nbLivres) }}</div>
        <div class="col-md-3">{{ form_row(form.pays) }}</div>
    </div>
    <div class="row">
        <div class="col-md-3">{{ form_row(form.nomMus) }}</div>
    </div>
    {{ form_rest(form) }}
        <button class="btn btn-primary">{{ button_label|default('Enregistrer') }}</button>
    {{ form_end(form) }}
</div>
